@page
@model HospitalManagement.Pages.Patient.ListModel
@{
}
<form method="post" class="shadow">
 
         <h6 class="text-danger">@Html.ValidationSummary()</h6>
         <h2>Patient List</h2>
    </div>
     
    

    <table class="table">
        <thead>
            <tr>
                <th scope="col">Id</th>
                <th scope="col">FirstName</th>
                <th scope="col">LastName</th>
                <th scope="col">Gender</th>
                <th scope="col">Dob</th>
                <th scope="col">MobileNumber</th>
                <th scope="col">Aadhar Number</th>
                <th scope="col">State</th>
                <th scope="col">City</th>
                <th scope="col">Street</th>
            </tr>
        </thead>
        <tbody>
            @foreach(var p in @Model.Patients)
            {
                <tr>
                    <td>@p.Id</td>
                    <td>@p.FirstName</td>
                    <td>@p.LastName</td>
                    <td>@p.Gender</td>
                    <td>@p.Dob.ToString("yyyy-MM-dd")</td>
                    <td>@p.MobileNumber</td>
                    <td>@p.AadharNo</td>
                    <td>@p.State</td>
                    <td>@p.City</td>
                    <td>@p.Street</td>
                    
         
                    
                    
                </tr>
            }
        </tbody>

    </table>
</form>
