@page "{id}"
@model HospitalManagement.Pages.Patients.vaccineResultModel
@{
}
<form method="post" class="shadow">
 
         <h6 class="text-danger">@Html.ValidationSummary()</h6>
         <h2> Vaccination Status</h2>
    </div>
     
    
    <table class="table">
        <thead>
            <tr>
                
                <th scope="col">FirstName</th>
                <th scope="col">Aadhar Number</th>
                <th scope="col">MobileNumber</th>
                <th scope="col">Gender</th>
               <th scope="col">State</th>
                <th scope="col">HospitalName</th>
                <th scope="col">VaccineName</th>
                <th scope="col">AppointmentDate</th>
                <th scope="col">VaccinatedDate</th>
               
                <th scope="col">Result</th>
                
            </tr>
        </thead>
        <tbody>
           
                <tr>
                    
                    <td>@Model.Vaccine.FirstName</td>
                    <td>@Model.Vaccine.AadharNo</td>
                    <td>@Model.Vaccine.MobileNumber</td>
                    <td>@Model.Vaccine.Gender</td>
                    <td>@Model.Vaccine.State</td>
                    <td>@Model.Vaccine.HospitalName</td>
                    <td>@Model.Vaccine.VaccineName</td>
                     <td>@Model.Vaccine.AppointmentDate.ToString("yyyy-MM-dd")</td>
                     <td>@Model.Vaccine.VaccinatedDate?.ToString("yyyy-MM-dd")</td>
                    <td>@Model.Vaccine.Status</td>
                    
                   
                    
                    
                </tr>
            
        </tbody>
          
    </table>
     <div class="col-sm-6">
                    <a href="/Patients/Profile" class="btn btn-danger col-sm-6" role="button">Back</a>
                </div>
</form>
